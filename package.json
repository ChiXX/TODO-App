{
  "name": "mongodbcrud",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "mongo": "docker compose -f docker-compose.db.yml up -d",
    "dev": "npm run mongo && next dev",
    "seed": "node seed.js",
    "reset-db": "docker compose -f docker-compose.db.yml down -v && docker compose -f docker-compose.db.yml up -d",
    "dev:seed": "npm run reset-db && npm run seed && next dev",
    "dev:reset": "npm run reset-db && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "eslint": "8.43.0",
    "eslint-config-next": "13.4.6",
    "lodash.debounce": "^4.0.8",
    "mongoose": "^7.3.0",
    "next": "13.4.6",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.9.0",
    "swagger-ui": "^5.21.0",
    "swagger-ui-react": "^5.21.0",
    "tailwindcss": "3.3.2"
  }
}
